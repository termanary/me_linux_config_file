#the next is what I define myself->me3

alias l='ls --color=auto '
alias la='ls -A --color=auto '
alias lh='ls -lh --color=auto '

alias c='cd '

alias rm='rm -v '
alias cp='cp -iv '
alias mv='mv -iv '

LANG=en_US.UTF-8
LANGUAGE=en_US.UTF-8
LC_ALL=en_US.UTF-8

alias v='vim ' 
alias e='emacs '
alias gdb='gdb -tui --command=~/.gdb '
alias s='screen '
alias w3m='w3m google.com/ncr '

